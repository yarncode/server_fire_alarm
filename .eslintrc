{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		// "indent": ["error", "tab"],
		"quotes": ["error", "single"],
		"no-unused-vars": "off",
		"no-empty-function": "warn"
	}
}
